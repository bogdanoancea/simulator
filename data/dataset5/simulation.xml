r<simulation>
        <start_time>0</start_time>
        <end_time>2400</end_time>
		<time_increment>1</time_increment>
		<time_stay distributionType = "Normal">
			<mean>20</mean>
			<sd>2</sd>
		</time_stay>
        <interval_between_stays distributionType = "Exponential">
        	<mean>120</mean>
        </interval_between_stays>
		<mno>
        	<mno_name>MNO1</mno_name>
        	<prob_mobile_phone>0.35</prob_mobile_phone>
		</mno>
		<prob_sec_mobile_phone>0.15</prob_sec_mobile_phone>
		<movement_type>home_work</movement_type>
		<specs_home_work>
			<home>
				<x_home>2000</x_home>
				<y_home>7500</y_home>
				<x_sd_home>1000</x_sd_home>
				<y_sd_home>1000</y_sd_home>
			</home>
			<home>
				<x_home>8000</x_home>
				<y_home>7500</y_home>
				<x_sd_home>750</x_sd_home>
				<y_sd_home>750</y_sd_home>
			</home>
			<work>
				<x_work>2500</x_work>
				<y_work>8500</y_work>
				<x_sd_work>750</x_sd_work>
				<y_sd_work>750</y_sd_work>
			</work>
			<work>
				<x_work>5000</x_work>
				<y_work>2000</y_work>
				<x_sd_work>750</x_sd_work>
				<y_sd_work>750</y_sd_work>
			</work>
			<percent_time_home>0.5</percent_time_home>
			<percent_time_work>0.3</percent_time_work>
			<percent_time_travel>0.03</percent_time_travel>
			<direction_angle_distribution type = "Laplace">
					<scale>15</scale>
			</direction_angle_distribution>
		</specs_home_work>
		<connection_type>strength</connection_type>
		<event_type>cellIDTA</event_type>
		<conn_threshold>-85</conn_threshold>
		<grid_file>grid.csv</grid_file>
		<grid_dim_tile_x>250</grid_dim_tile_x>
		<grid_dim_tile_y>250</grid_dim_tile_y>
		<persons_file>persons.csv</persons_file>
		<antennas_file>antennas.csv</antennas_file>
		<random_seed>12</random_seed>
		<output_dir>output2</output_dir>
</simulation>