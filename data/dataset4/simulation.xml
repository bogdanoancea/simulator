 <simulation>
	<start_time>0</start_time>
	<end_time>200</end_time>
	<time_increment>1</time_increment>
	<time_stay distributionType = "Normal">
		<mean>10</mean>
		<sd>1</sd>
	</time_stay>
    <interval_between_stays distributionType = "Exponential">
      	<mean>25</mean>
    </interval_between_stays>
	<mno>
		<mno_name>MNO1</mno_name>
		<prob_mobile_phone>0.35</prob_mobile_phone>
	</mno>
	<prob_sec_mobile_phone>0.15</prob_sec_mobile_phone>
	<movement_pattern type="random_walk_closed_map_drift">
		<trend_angle_1_distribution distributionType = "Normal">
			<mean>135</mean>
			<sd>20</sd>
		</trend_angle_1_distribution>
		<trend_angle_2_distribution distributionType = "Normal">>
			<mean>225</mean>
			<sd>20</sd>
		</trend_angle_2_distribution>
		<return_angle_distribution distributionType = "Uniform">
			<min>120</min>
			<max>240</max>
		</return_angle_distribution>
	</movement_pattern>
	<connection_type>quality</connection_type>
	<qual_min>0.3</qual_min>
	<grid_file>grid.csv</grid_file>
	<grid_dim_tile_x>500</grid_dim_tile_x>
	<grid_dim_tile_y>500</grid_dim_tile_y>
	<persons_file>persons.csv</persons_file>
	<antennas_file>antennas.csv</antennas_file>
	<random_seed>12</random_seed>
</simulation>