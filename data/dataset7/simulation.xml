<simulation>
        <start_time>0</start_time>
        <end_time>2400</end_time>
		<time_increment>1</time_increment>
		<time_stay_distribution type = "Normal">
			<mean>20</mean>
			<sd>2</sd>
		</time_stay_distribution>
        <interval_between_stays_distribution type = "Exponential">
        	<mean>120</mean>
        </interval_between_stays_distribution>
		<mno>
        	<mno_name>MNO1</mno_name>
        	<prob_mobile_phone>0.35</prob_mobile_phone>
		</mno>
		<prob_sec_mobile_phone>0.15</prob_sec_mobile_phone>
		<movement_pattern type = "home_work_manhattan">
			<home>
				<x>2000</x>
				<y>7500</y>
				<x_sd>1000</x_sd>
				<y_sd>1000</y_sd>
			</home>
			<home>
				<x>8000</x>
				<y>7500</y>
				<x_sd>750</x_sd>
				<y_sd>750</y_sd>
			</home>
			<work>
				<x>2500</x>
				<y>8500</y>
				<x_sd>750</x_sd>
				<y_sd>750</y_sd>
			</work>
			<work>
				<x>5000</x>
				<y>2000</y>
				<x_sd>750</x_sd>
				<y_sd>750</y_sd>
			</work>
			<anchor_point>
				<x>8000</x>
				<y>4000</y>
				<x_sd>700</x_sd>
				<y_sd>700</y_sd>
			</anchor_point>
			<anchor_point>
				<x>3000</x>
				<y>5000</y>
				<x_sd>700</x_sd>
				<y_sd>700</y_sd>
			</anchor_point>
			<manhattan_grid>
				<x_step>50</x_step>
				<y_step>50</y_step>
				<x_origin>0</x_origin>
				<y_origin>0</y_origin>
			</manhattan_grid>
			<percent_time_home>0.5</percent_time_home>
			<percent_time_work>0.3</percent_time_work>
			<percent_time_anchor_point>0.1</percent_time_anchor_point>
			<percent_time_travel>0.03</percent_time_travel>
			<prob_anchor_point>0.8</prob_anchor_point>
		</movement_pattern>
		<connection_type>strength</connection_type>
		<event_type>cellIDTA</event_type>
		<conn_threshold>-85</conn_threshold>
		<grid_file>grid.csv</grid_file>
		<grid_dim_tile_x>250</grid_dim_tile_x>
		<grid_dim_tile_y>250</grid_dim_tile_y>
		<persons_file>persons.csv</persons_file>
		<antennas_file>antennas.csv</antennas_file>
		<random_seed>12</random_seed>
		<output_dir>output2</output_dir>
</simulation>